new WOW().init();$('.close-cookie').click(function(e){e.preventDefault();setCookie("websitecookie","dfcookiepolicy",30);$('.cookie-banner').hide()});var cookieresult=getCookie("websitecookie");console.log(cookieresult);if(cookieresult=="dfcookiepolicy"){$('.cookie-banner').hide()}else{$('.cookie-banner').show()}
function setCookie(cname,cvalue,exdays){var d=new Date();d.setTime(d.getTime()+(exdays*24*60*60*1000));var expires="expires="+d.toUTCString();document.cookie=cname+"="+cvalue+";"+expires+";path=/"}
function getCookie(cname){var name=cname+"=";var decodedCookie=decodeURIComponent(document.cookie);var ca=decodedCookie.split(';');for(var i=0;i<ca.length;i++){var c=ca[i];while(c.charAt(0)==' '){c=c.substring(1)}
if(c.indexOf(name)==0){return c.substring(name.length,c.length)}}
return""}
